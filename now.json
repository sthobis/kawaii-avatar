{
  "name": "kawaii-avatar",
  "alias": "kawaii-avatar",
  "version": 2,
  "builds": [
      { "src": "/www/package.json", "use": "@now/static-build" },
      { "src": "/api/*.js", "use": "@now/node", "config": { "maxLambdaSize": "40mb" } },
      { "src": "/static/*", "use": "@now/static" }
  ],
  "routes": [
      {
        "src": "/", "dest": "/www"
      },
      {
        "src": "/favicon.ico",
        "dest": "/static/favicon.ico"
      },
      {
        "src": "/(.*)",
        "dest": "/api/avatar.js"
      }
  ]
}
